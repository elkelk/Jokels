$("#loading").remove();
$('#joke[data-sequence="<%=@sequence_identifier%>"]').html("");
$('#joke[data-sequence="<%=@sequence_identifier%>"]').html("<%= escape_javascript(render :partial => 'home/joke', :locals => {:joke => @joke, :refresh => true, :min => false, :sequence => true, :feed_path => feed_user_path(@user) + '?type=' + @type}) %>");
$('#joke[data-sequence="<%=@sequence_identifier%>"]').slideDown("fast");
$('#joke[data-sequence="<%=@sequence_identifier%>"]').ready(function(){
	$(this).find('#click-for-punchline, #question').click(enable_joke_stuff);
	document.title = "<%= @title %>";
})

