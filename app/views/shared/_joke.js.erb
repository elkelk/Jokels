$("#loading").remove();
$("#joke").html("");
$("#joke").html("<%= escape_javascript(render :partial => 'home/joke', :locals=>{:joke => @joke, :refresh => refresh, :min => false, :sequence => false}) %>");
$("#joke").slideDown("fast");
$("#joke").ready(function(){
	$('#click-for-punchline, #question').click(enable_joke_stuff);
	document.title = "<%= @title %>";
});
